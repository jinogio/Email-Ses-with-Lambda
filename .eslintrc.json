{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "error",
    "semi": [2, "always"],
    "comma-dangle": "off",
    "camelcase": "off",
    "space-before-function-paren": "off"
  }
}
